<p id="notice"><%= notice %></p>

<h1>Listing Semisters</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Semister name</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @semisters.each do |semister| %>
      <tr>
        <td><%= semister.course_id %></td>
        <td><%= semister.semister_name %></td>
        <td><%= semister.year %></td>
        <td><%= link_to 'Show', semister %></td>
        <td><%= link_to 'Edit', edit_semister_path(semister) %></td>
        <td><%= link_to 'Destroy', semister, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Semister', new_semister_path %>
